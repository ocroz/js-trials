{
  "name": "unless-fetch",
  "version": "1.0.0",
  "description": "alternatives solutions to fetch",
  "license": "ISC",
  "repository": {
    "type": "git"
  },
  "browser": {
    "./src/jira/env/index.js": "./src/jira/env/browser.js",
    "./src/vone/env/index.js": "./src/vone/env/browser.js"
  },
  "scripts": {
    "jira:build": "browserify src/jira/calls.js -o dist/jira-calls-bundle.js",
    "jira:deploy": "cp dist/* //safetest.hq.k.grp/custom/tmp/",
    "jira:start:browser": "chrome https://safetest.hq.k.grp/Safetest/custom/tmp/jira-calls.html",
    "jira:start:node": "node src/jira/errors.js",
    "vone:build": "browserify src/vone/errors.js -o dist/vone-calls-bundle.js",
    "vone:deploy": "cp dist/* //safetest.hq.k.grp/custom/tmp/",
    "vone:start:browser": "chrome https://safetest.hq.k.grp/Safetest/custom/tmp/vone-calls.html",
    "vone:start:node": "node src/vone/errors.js"
  },
  "dependencies": {
    "browserify": "^14.5.0",
    "httpntlm": "^1.7.5",
    "node-fetch": "^1.7.3"
  }
}
